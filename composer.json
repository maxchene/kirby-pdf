{
    "name": "maxchene/kirby-pdf",
    "homepage": "https://github.com/maxchene/kirby-pdf",
    "description": "Kirby plugin that a PDF engine to convert HTML to PDF",
    "license": "MIT",
    "type": "kirby-plugin",
    "version": "1.0.2",
    "authors": [
        {
            "name": "Maxime CHENE",
            "email": "maxime@kaliel.fr"
        }
    ],
    "require": {
        "php": ">=8.1",
        "getkirby/composer-installer": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Maxchene\\KirbyPdf\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "allow-plugins": {
            "getkirby/composer-installer": true
        }
    },
    "extra": {
        "installer-name": "pdf"
    },
    "keywords": [
        "kirby",
        "kirby3",
        "kirby3-cms",
        "kirby3-plugin",
        "pdf",
        "wkhtmltopdf",
        "dompdf",
        "mpdf",
        "tcpdf"
    ],
    "require-dev": {
        "symfony/var-dumper": "^6.2",
        "getkirby/cli": "^1.1"
    }
}
